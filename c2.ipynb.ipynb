        "#include<stdio.h>\n",
        "\n",
        "// Function to calculate factorial using recursion\n",
        "int factorial(int n)\n",
        "{\n",
        "    if (n == 0)\n",
        "        return 1;\n",
        "    else\n",
        "        return n * factorial(n - 1);\n",
        "}\n",
        "int main()\n",
        "{\n",
        "    int num = 5;\n",
        "    printf(\"Factorial of %d is %d\", num, factorial(num));\n",
        "    return 0;\n",
        "}\n",
        "\n"
      ]
